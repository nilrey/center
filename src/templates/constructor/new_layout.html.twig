          <li id="tab_s_finance"><a href="{{ path('s_finance', { 'id_reg': oiv.Id }) }}">Финансы</a></li>
          <li id="tab_s_banking"><a href="{{ path('s_banking', { 'id_reg': oiv.Id }) }}">Банковская система</a></li>
      </ul>

      <ul id="block_social_info" class="psp-menu-left displayNone">
          <li id="tab_s_demography"><a href="{{ path('s_demography', { 'id_reg': oiv.Id }) }}">Демография</a></li>
          <li id="tab_s_healthcare"><a href="{{ path('s_healthcare', { 'id_reg': oiv.Id }) }}">Здравоохранение</a></li>
          <li id="tab_s_education"><a href="{{ path('s_education', { 'id_reg': oiv.Id }) }}">Образование</a></li>
          <li id="tab_s_living_conditions"><a href="{{ path('s_living_conditions', { 'id_reg': oiv.Id }) }}">Уровень жизни населения</a></li>
          <li id="tab_s_housing_conditions"><a href="{{ path('s_housing_conditions', { 'id_reg': oiv.Id }) }}">Жилищные условия населения</a></li>
