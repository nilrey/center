          <li id="tab_s_investments"><a href="{{ path('s_investments', { 'id_reg': oiv.Id }) }}">Инвестиции в основной капитал</a></li>
          <li id="tab_s_consumers"><a href="{{ path('s_consumers', { 'id_reg': oiv.Id }) }}">Потребительский рынок</a></li>
          <li id="tab_s_prices"><a href="{{ path('s_prices', { 'id_reg': oiv.Id }) }}">Цены</a></li>
