                        <li id="tab_s_education"><a href="('s_education', { 'id_reg': oiv.Id }) }}">Образование</a></li>
                        <li id="tab_s_living_conditions"><a href="('s_living_conditions', { 'id_reg': oiv.Id }) }}">Уровень жизни населения</a></li>
                        <li id="tab_s_housing_conditions"><a href="('s_housing_conditions', { 'id_reg': oiv.Id }) }}">Жилищные условия населения</a></li>
