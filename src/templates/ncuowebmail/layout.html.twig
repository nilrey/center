{% extends 'layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/ncuoportal/css/custom.tabs.foiv.css') }}" />
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/ncuoportal/css/region.css') }}" />
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/ncuoportal/css/region_custom.css') }}" />
{% endblock %}  

{% block javascripts %}
    {{ parent() }}
    
    <script type="text/javascript" src="{{asset('bundles/ncuofoiv/js/DialogHelpers.js')}}"></script>
    <script  src="{{ asset('bundles/ncuofoiv/js/foivShow.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ncuoportal/js/datatables/jquery.dataTables.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ncuoportal/js/datatables/dataTables.bootstrap.js') }}"></script>
    <script type="text/javascript" src="{{asset('bundles/ncuofoiv/js/foiv.js') }}"></script>
    <!-- <script type="text/javascript" src="{{asset('bundles/ncuofoiv/js/DeleteItemsHelpers.js') }}"></script> -->
    <script type="text/javascript" src="{{asset('bundles/ncuofoiv/js/onDeleteTableRecord.js') }}"></script>
    <script type="text/javascript" src="{{asset('bundles/ncuofoiv/js/foiv_tab_menu.js') }}"></script>
    <script src="{{ asset('bundles/ncuoportal/js/oiv/functions.js') }}"></script>
    
    <script>
        var DATATABLES_RU_JSON = '{{ asset('bundles/ncuoportal/js/datatables/ru_Ru.json') }}';
        var DATATABLES_RU_REG_LIST_JSON = '{{ asset('bundles/ncuoportal/js/datatables/ru_Ru_reg_list.json') }}';
    </script>
      
{% endblock %}  

{% block breadcrumb %}
    
    <li><a href="('region') }}">Паспорта ФОИВ РФ / Субьектов РФ</a></li>
    <li><a href="('region') }}">Субьекты РФ</a></li>
    {% if oiv is defined %}
    <li><a href="('region_show', { 'id_reg': oiv.Id }) }}">{{ oiv.name }}</a></li>
    {%endif %}
{% endblock %}    

{% block content %}
    
        {% if oiv.name is defined %}<h4>
    <span class="foiv-logo">{% autoescape %}{{ oiv.heraldic_img|raw }}{% endautoescape %}</span>
        {{ oiv.name }}
        {% endif %}
    </h4>
    <div class="row-fluid">
        <div class="span12">
            <ul class="nav nav-tabs foiv-tabs">
                <li id="tab_common_info" class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        Общие сведения <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li id="tab_s_geo_position"><a href="('s_geo_position', { 'id_reg': oiv.Id }) }}">Административно-географическое положение</a></li>
                        <li id="tab_s_adm_structure"><a href="('s_adm_structure', { 'id_reg': oiv.Id }) }}">Административно-территориальное устройство субъекта РФ</a></li>
                        <li id="tab_s_gov_structure"><a href="('s_gov_structure', { 'id_reg': oiv.Id }) }}">Органы государственной власти и местного самоуправления</a></li>
                    </ul>
                </li>
                <li id="tab_economic_info" class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        Экономическое развитие <span class="caret"></span>
